{
    "name": "ivinteractive/laravel-rotation",
    "description": "A package to simplify data decryption and re-encryption when the APP_KEY needs rotation.",
    "keywords": [
        "ivinteractive",
        "laravel-rotation"
    ],
    "homepage": "https://github.com/ivinteractive/laravel-rotation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Craig Spivack",
            "email": "cs@ivinteractive.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/horizon": "^5.7",
        "laravel/slack-notification-channel": "^2.4",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.0|^10.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "IvInteractive\\Rotation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IvInteractive\\Rotation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit"
        ],
        "cs": "vendor/bin/php-cs-fixer fix --diff",
        "phpstan": "vendor/bin/phpstan analyse src --level=6"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IvInteractive\\Rotation\\RotationServiceProvider"
            ]
        }
    }
}
