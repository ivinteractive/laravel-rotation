{
    "name": "ivinteractive/laravel-rotation",
    "description": "A package to simplify data decryption and re-encryption when the APP_KEY needs rotation.",
    "keywords": [
        "ivinteractive",
        "laravel-rotation"
    ],
    "homepage": "https://github.com/ivinteractive/laravel-rotation",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Craig Spivack",
            "email": "cs@ivinteractive.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ivinteractive\\LaravelRotation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivinteractive\\LaravelRotation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ivinteractive\\LaravelRotation\\LaravelRotationServiceProvider"
            ],
            "aliases": {
                "LaravelRotation": "Ivinteractive\\LaravelRotation\\LaravelRotationFacade"
            }
        }
    }
}
